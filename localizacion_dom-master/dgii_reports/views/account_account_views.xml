<odoo>

    <record id="view_account_form_inherited" model="ir.ui.view">
        <field name="name">view.account.form.inherited</field>
        <field name="model">account.account</field>
        <field name="inherit_id" ref="account.view_account_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='allowed_journal_ids']" position="after">
                <field name="box_attachment_a"/>
                <field name="box_it1"/>
                <field name="account_fiscal_type" invisible="1"/>
                <field name="isr_retention_type"/>
            </xpath>
        </field>
    </record>

</odoo>
